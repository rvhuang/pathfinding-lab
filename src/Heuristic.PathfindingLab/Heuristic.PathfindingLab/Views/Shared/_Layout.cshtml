@using Microsoft.AspNetCore.Http
@using Microsoft.AspNetCore.Http.Extensions

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta property="og:title" content="Pathfinding Laboratory - Powered by LINQ to A*" />
    <meta property="og:description" content="Run, test and compare all algorithms and heuristic functions" />
    <meta property="og:image" content="@UriHelper.BuildAbsolute(Context.Request.Scheme, Context.Request.Host, new PathString("/images/assets.png"))" />
    <meta property="og:url" content="@UriHelper.BuildAbsolute(Context.Request.Scheme, Context.Request.Host)">
    <title>Pathfinding Laboratory - Powered by LINQ to A*</title>
    <link rel="shortcut icon" href="data:image/png;base64," type="image/png" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/css/bootstrap.min.css" />
    @RenderSection("Styles", false)
</head>
<body>
    <a href="https://github.com/rvhuang/pathfinding-lab" target="_blank">
        <img style="position: absolute; top: 0; right: 0; border: 0; z-index: 1" src="https://s3.amazonaws.com/github/ribbons/forkme_right_darkblue_121621.png" alt="Fork me on GitHub" />
    </a>
    <div class="container-fluid">
        <div class="page-header">
            <h1>Pathfinding Laboratory&nbsp;
                <small>powered by <a href="https://github.com/rvhuang/linq-to-astar" target="_blank">LINQ to A*</a></small>
            </h1>
            <p>Run, test and compare all algorithms and heuristic functions</p>
        </div>
        @RenderBody()
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/js/bootstrap.min.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
